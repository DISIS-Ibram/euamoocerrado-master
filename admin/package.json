  "scripts": {
    "start": "webpack-dev-server --inline --hot --config webpack.dev.config.js --history-api-fallback",
    "build-dev": "webpack --config webpack.dll.js; webpack --config webpack.prod.config.js",
    "build-prod": "webpack -p --config webpack.dll.prod.js; webpack -p --config webpack.prod.config.js",
    "test": "mocha --compilers js:babel-register --recursive",
    "test-watch": "npm test -- --watch",
    "size": "webpack --profile --json > stats.json",
    "build:dll": "webpack --config webpack.dll.js",
    "gulp-watch": "gulp watch",
    "deploy": "rsync -av --exclude .git --exclude node_modules ./public/ euamocerradocom@euamocerrado.com.br:~/admin.euamocerrado.com.br/",
    "analyze": "webpack --config webpack.dll.prod.js --json > dll.json;webpack --config webpack.prod.config.js --json > bundle.json;"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-regenerator",
      "transform-runtime"
