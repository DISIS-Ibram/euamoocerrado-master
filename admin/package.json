{
  "name": "admin-euamocerrado",
  "version": "1.0.0",
  "description": "Sistemas de Inform",
  "keywords": [
    "react",
    "redux",
    "minimal",
    "starter kit",
    "boilerplate"
  ],
  "main": "index.js",
  "homepage": "http://redux-minimal.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/catalin-luntraru/redux-minimal"
  },
  "scripts": {
    "start": "webpack serve --hot --config webpack.dev.config.js --history-api-fallback",
    "build-dev": "webpack --config webpack.dll.js; webpack --config webpack.prod.config.js",
    "build-prod": "webpack -p --config webpack.dll.prod.js; webpack -p --config webpack.prod.config.js",
    "test": "mocha --compilers js:babel-register --recursive",
    "test-watch": "npm test -- --watch",
    "size": "webpack --profile --json > stats.json",
    "build:dll": "webpack --config webpack.dll.js",
    "gulp-watch": "gulp watch",
    "deploy": "rsync -av --exclude .git --exclude node_modules ./public/ euamocerradocom@euamocerrado.com.br:~/admin.euamocerrado.com.br/",
    "analyze": "webpack --config webpack.dll.prod.js --json > dll.json;webpack --config webpack.prod.config.js --json > bundle.json;"
  },
  "author": "Catalin Luntraru",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "denormalizr": "^0.6.0",
    "faker": "^5.5.3",
    "fuse.js": "^7.1.0",
    "fuzzysearch": "^1.0.3",
    "geodesy": "^2.4.0",
    "humps": "^2.0.1",
    "normalizr": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.57.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.23.1",
    "react-toastify": "^11.0.5",
    "redux": "^4.2.1",
    "redux-api": "^0.12.0",
    "redux-thunk": "^2.4.2",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "topojson-client": "^3.1.0",
    "updeep": "^1.2.4",
    "util": "^0.12.5",
    "wkx": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@svgr/webpack": "^8.1.0",
    "babel-loader": "^10.0.0",
    "config": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "lodash-webpack-plugin": "^0.11.6",
    "nib": "^1.2.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
